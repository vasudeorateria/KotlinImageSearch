<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/galleryFragment2">
    <fragment
        android:id="@+id/galleryFragment2"
        android:name="com.codinginflow.imagesearchapp.ui.gallery.GalleryFragment"
        android:label="Gallery"
        tools:layout="@layout/frgment_gallery" >
        <action
            android:id="@+id/action_galleryFragment2_to_details"
            app:destination="@id/details" />
    </fragment>
    <fragment
        android:id="@+id/details"
        android:name="com.codinginflow.imagesearchapp.ui.details.Details"
        android:label="Photo"
        tools:layout="@layout/fragment_details" >
        <argument
            android:name="photo"
            app:argType="com.codinginflow.imagesearchapp.data.UnsplashPhoto" />
    </fragment>
</navigation>